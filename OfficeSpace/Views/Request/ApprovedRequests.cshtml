@{
    string array = "[6, 12, ALL]";

}
@model OfficeSpace.Models.RequestModel
@{
    ViewBag.Title = "ApprovedRequests";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<link href="~/Content/bootstrap-table.min.css" rel="stylesheet" />
<link href="~/ContentOld/Theme/Spinner.css" rel="stylesheet" />
<style>
    .fixed-table-toolbar .columns-right {
        margin-top: 0px !important;
    }

    .fixed-table-toolbar .search {
        margin-top: 0px !important;
    }

    .fixed-table-toolbar .columns label {
        padding: 0px !important;
        padding-left: 10px !important;
    }

    table tr {
        cursor: pointer;
    }

    table td {
        white-space: nowrap;
        overflow: hidden;
    }
</style>


<div id="page-wrapper">
    <div class="panel">
        <div>
            <div class="panel-heading red-background-colour">
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-4">
                        <h4><strong>Approved Requests </strong></h4>
                    </div>
                    <div class="col-lg-8 col-md-8 col-sm-8"></div>
                </div>
            </div>
            @*<div class="panel-body tableLoader" style="text-align:left;">*@

            @using (Html.BeginForm("SearchApprovedRequests", "Request", FormMethod.Post, new { id = "FurnishedRentals", @class = "bs-example " }))
            {


                @*<div class="table-responsive" style="overflow-y:scroll;">*@
                <table id="tblRentalDetails" class="table table-striped table-bordered table-sm " cellspacing="0" width="100%"
                       data-toggle="table"
                       data-toolbar="#toolbar"
                       data-search="false"
                       data-show-columns="false"
                       data-minimum-count-columns="2"
                       data-pagination="true"
                       data-id-field="ID"
                       data-page-size="@ViewBag.pageSize"
                       data-page-list="@array"
                       data-show-footer="false"
                       data-row-style="rowStyle"
                       @*data-fixed-columns="true"
                                   data-fixed-columns-left="4"
                                   data-fixed-columns-right="0"*@>
                    <thead>
                        <tr class="table-heading-font">
                            <th data-sortable="false">SL. No.</th>
                            <th data-sortable="false">Creation Date</th>
                            <th data-sortable="false">Company</th>
                            <th data-sortable="false">City</th>
                            <th data-sortable="false">Request Type</th>
                            <th data-sortable="false">Bussiness Type</th>
                            <th data-sortable="false"> Allocation Type  </th>
                            <th data-sortable="false">
                                Fitouts / Bareshell
                            </th>
                            <th data-sortable="false">Data Type</th>
                            <th data-sortable="false">Office Name</th>
                            <th data-sortable="false">
                                Office Location
                            </th>


                            <th data-sortable="false">
                                Lease Start Date
                            </th>
                            <th data-sortable="false">
                                Lease Starting Rental Amount (INR)
                            </th>
                            <th data-sortable="false">
                                Rental Escalation
                            </th>
                            <th data-sortable="false" style="text-wrap:none;width:50px">
                                Escalation Period
                            </th>
                            <th data-sortable="false">
                                Lease Period (In Years)
                            </th>
                            @*<th data-sortable="false">
                                Lease Closure Date
                            </th>*@

                            <th data-sortable="false">
                                Advance Rental Paid (INR)
                            </th>
                            <th data-sortable="false">
                                Total Amount onhold with owner
                            </th>


                            <th data-sortable="false">
                                Notice Period (In Months)
                            </th>
                            <th data-sortable="false">
                                Signage covering Roads
                            </th>
                            <th data-sortable="false">
                                Number of Employee
                            </th>
                            <th data-sortable="false">
                                Super BuiltUp Area (Sq.Ft.)
                            </th>
                            <th data-sortable="false">
                                BuiltUp Area (Sq.Ft.)
                            </th>
                            <th data-sortable="false">
                                Carpet Area (Sq.Ft.)
                            </th>
                            <th data-sortable="false">
                                Rental Area (Sq.Ft.)
                            </th>

                            <th data-sortable="false">
                                Rental Cost (INR/ Sq.Ft)
                            </th>

                            <th data-sortable="false">
                                Security Deposit  (INR)
                            </th>

                            <th data-sortable="false">
                                Present Monthly Rental Cost (INR)
                            </th>
                            <th data-sortable="false">
                                Present Monthly Billing (INR)
                            </th>
                            <th data-sortable="false">
                                Rental Cost % to Billing
                            </th>
                            <th data-sortable="false">
                                Monthly Maintenance Cost (INR)
                            </th>
                            <th data-sortable="false">
                                Avg Monthly Maintenance Cost (INR)
                            </th>
                            <th data-sortable="false">
                                Monthly Electricity Cost(INR)
                            </th>
                            <th data-sortable="false">
                                Monthly All_other Costs(INR)
                            </th>
                            <th data-sortable="false">
                                Total_Monthly Rental Costs (INR)
                            </th>
                            <th data-sortable="false">
                                Number of_Car Parking
                            </th>

                            <th data-sortable="false">
                                Name
                            </th>
                            <th data-sortable="false">
                                Email
                            </th>
                            <th data-sortable="false">
                                Mobile
                            </th>
                            <th data-sortable="false">
                                Remarks
                            </th>


                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var detail in Model.FurnishedDataList)
                        {
                            var indexing = Model.FurnishedDataList.IndexOf(detail);

                            <tr>
                                @Html.HiddenFor(m => m.FurnishedDataList[indexing].ID)
                                <td> @detail.RequestID </td>
                                <td> @detail.CreationDate </td>
                                <td> @detail.Company </td>
                                <td> @detail.City   </td>
                                <td> @detail.SelectedMenu   </td>
                                <td> @detail.BuisnessType  </td>
                                <td> @detail.AllocationType  </td>
                                <td> @detail.Fitouts   </td>
                                <td> @detail.DataType   </td>

                                @if (detail.DataType == "Existing")
                                {
                                    <td> @detail.ExistingOfficeName  </td>
                                    <td> @detail.ExistingLocation  </td>
                                    <td> @detail.ExistingLeaseStartDate  </td>
                                    <td> @detail.ExistingLeaseStartAmount </td>
                                    <td> @detail.ExistingRentalEscallation </td>
                                    <td> @detail.ExistingEscallationPeriod </td>
                                    <td> @detail.ExistingLeasePeriod   </td>
                                    @*<td> @detail.ExistingLeaseClosureDate   </td>*@
                                    <td> @detail.ExistingAdvanceRental </td>
                                    <td> @detail.ExistingAmtHoldWithOwner </td>
                                    <td> @detail.ExistingNoticePeriod  </td>
                                    <td> @detail.Signage  </td>
                                    <td> @detail.NoOfPersons  </td>
                                    <td> @detail.SuperBuiltUp  </td>
                                    <td> @detail.BuiltUp </td>
                                    <td> @detail.CarpetArea </td>
                                    <td> @detail.RentalArea </td>
                                    <td> @detail.CalcMonthlyCost   </td>
                                    <td> @detail.SecurityDeposit  </td>
                                    <td> @detail.ExistingPresentMonthlyRenatlCost   </td>
                                    <td> @detail.ExistingPresentMonthlyBilling   </td>
                                    <td> @detail.ExistingRentalCostPer </td>
                                    <td> @detail.ExistingMonthlyMaintenenceCost   </td>
                                    <td> @detail.ExistingAvgMaintenanceCost </td>
                                    <td> @detail.ExistingmonthlyElectricCost  </td>
                                    <td> @detail.ExistingMonthlyOtherCosts </td>
                                    <td> @detail.ExistingTotalMonthlyRenatlCost  </td>
                                    <td> @detail.ExistingCarPark </td>
                                    <td> @detail.ExistingName  </td>
                                    <td> @detail.ExistingEmail  </td>
                                    <td> @detail.ExistingMobile </td>
                                    <td> @detail.Remarks </td>
                                }
                                else if (detail.DataType == "Proposed")
                                {
                                    <td> @detail.OfficeName</td>
                                    <td> @detail.ProposedLocation</td>
                                    <td> @detail.LeaseStartDate</td>
                                    <td> @detail.LeaseStartAmount</td>
                                            <td> @detail.RentalEscallation</td>
                                            <td> @detail.EscallationPeriod</td>
                                            <td> @detail.LeasePeriod</td>
                                            @*<td> @detail.LeaseClosureDate</td>*@
                                            <td> @detail.AdvanceRental</td>
                                            <td> @detail.AmtHoldWithOwner</td>
                                            <td> @detail.NoticePeriod</td>
                                            <td> @detail.ProposedSignage</td>
                                            <td> @detail.ProposedNoOfPersons</td>
                                            <td> @detail.ProposedSuperBuiltUp</td>

                                            <td> @detail.ProposedBuiltUp</td>
                                            <td> @detail.ProposedCarpetArea</td>
                                            <td> @detail.ProposedRentalArea</td>
                                            <td> @detail.ProposedCalcMonthlyCost</td>
                                            <td> @detail.ProposedSecurityDeposit</td>
                                            <td> @detail.PresentMonthlyRenatlCost</td>
                                            <td> @detail.PresentMonthlyBilling</td>
                                            <td> @detail.RentalCostPer</td>

                                            <td> @detail.MonthlyMaintenenceCost</td>
                                            <td> @detail.AvgMaintenanceCost</td>
                                            <td> @detail.monthlyElectricCost</td>
                                            <td> @detail.MonthlyOtherCosts</td>
                                            <td> @detail.TotalMonthlyRenatlCost</td>
                                            <td> @detail.ProposedCarPark</td>
                                            <td> @detail.Name</td>
                                            <td> @detail.Email</td>
                                            <td> @detail.Mobile</td>
                                            <td> @detail.ProposedRemarks</td>

                                }
                                else if (detail.DataType == "Initiated")
                                {
                                    <td> @detail.InitiateOfficeName</td>
                                    <td> @detail.InitiateLocation</td>
                                    <td> @detail.InitiateLeaseStartDate</td>
                                    <td> @detail.InitiateLeaseStartAmount</td>
                                    <td> @detail.InitiateRentalEscallation</td>
                                            <td> @detail.InitiateEscallationPeriod</td>
                                            <td> @detail.InitiateLeasePeriod</td>
                                            @*<td> @detail.InitiateLeaseClousureDate</td>*@
                                            <td> @detail.InitiateLeaseAdvanceRental</td>
                                            <td> @detail.InitiateAmtHoldWithOwner</td>
                                            <td> @detail.InitiateNoticePeriod</td>
                                            <td> @detail.InitiateSignage</td>
                                            <td> @detail.InitiateNoOfPerson</td>
                                            <td> @detail.InitiateSuperBuiltUp</td>

                                            <td> @detail.InitiateBuiltup</td>
                                            <td> @detail.InitiateCarpetArea</td>
                                            <td> @detail.InitiateRentalArea</td>
                                            <td> @detail.InitiateRentalCost</td>
                                            <td> @detail.InitiateSecurityDeposit</td>
                                            <td> @detail.InitiateMonthlyRentalCost</td>
                                            <td> @detail.InitiateMonthlyBilling</td>
                                            <td> @detail.InitiateRentalCostPer</td>

                                            <td> @detail.InitiatemonthlyMaintenanceCost</td>
                                            <td> @detail.InitiateAvgMaintenanceCost</td>
                                            <td> @detail.InitiateMonthlyElectricCost</td>
                                            <td> @detail.InitiateMonthlyOtherCosts</td>
                                            <td> @detail.InitiateTotalMonthlyRenatlCost</td>
                                            <td> @detail.InitiateCarpark</td>
                                            <td> @detail.InitiateName</td>
                                            <td> @detail.InitiateEmail</td>
                                            <td> @detail.InitiateMobile</td>
                                            <td> @detail.InitiateRemarks</td>

                                }
                                else
                                {
                                    <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedOfficeName_@indexing" name="FurnishedDataList[@indexing].IdentifiedOfficeName" value="@detail.IdentifiedOfficeName" maxlength="100" /></td>
                                    <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedLocation_@indexing" name="FurnishedDataList[@indexing].IdentifiedLocation" value="@detail.IdentifiedLocation" maxlength="100" /></td>
                                    <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedLeaseStartDate_@indexing" name="FurnishedDataList[@indexing].IdentifiedLeaseStartDate" value="@detail.IdentifiedLeaseStartDate" maxlength="100" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedLeaseStartAmount_@indexing" name="FurnishedDataList[@indexing].IdentifiedLeaseStartAmount" value="@detail.IdentifiedLeaseStartAmount" maxlength="10" type = "number" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedRentalEscallation_@indexing" name="FurnishedDataList[@indexing].IdentifiedRentalEscallation" value="@detail.IdentifiedRentalEscallation" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedEscallationPeriod@indexing" name="FurnishedDataList[@indexing].IdentifiedEscallationPeriod" value="@detail.IdentifiedEscallationPeriod" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedLeasePeriod_@indexing" name="FurnishedDataList[@indexing].IdentifiedLeasePeriod" value="@detail.IdentifiedLeasePeriod" maxlength="10" /></td>
                                            @*<td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedLeaseClousureDate_@indexing" name="FurnishedDataList[@indexing].IdentifiedLeaseClousureDate" value="@detail.IdentifiedLeaseClousureDate" maxlength="10" /></td>*@
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedLeaseAdvanceRental_@indexing" name="FurnishedDataList[@indexing].IdentifiedLeaseAdvanceRental" value="@detail.IdentifiedLeaseAdvanceRental" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedAmtHoldWithOwner_@indexing" name="FurnishedDataList[@indexing].IdentifiedAmtHoldWithOwner" value="@detail.IdentifiedAmtHoldWithOwner" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedNoticePeriod_@indexing" name="FurnishedDataList[@indexing].IdentifiedNoticePeriod" value="@detail.IdentifiedNoticePeriod" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedSignage_@indexing" name="FurnishedDataList[@indexing].IdentifiedSignage" value="@detail.IdentifiedSignage" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedNoOfPerson_@indexing" name="FurnishedDataList[@indexing].IdentifiedNoOfPerson" value="@detail.IdentifiedNoOfPerson" maxlength="800" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedSuperBuiltUp_@indexing" name="FurnishedDataList[@indexing].IdentifiedSuperBuiltUp" value="@detail.IdentifiedSuperBuiltUp" maxlength="10" /></td>

                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedBuiltup_@indexing" name="FurnishedDataList[@indexing].IdentifiedBuiltup" value="@detail.IdentifiedBuiltup" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedCarpetArea_@indexing" name="FurnishedDataList[@indexing].IdentifiedCarpetArea" value="@detail.IdentifiedCarpetArea" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedRentalArea_@indexing" name="FurnishedDataList[@indexing].IdentifiedRentalArea" value="@detail.IdentifiedRentalArea" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedRentalCost_@indexing" name="FurnishedDataList[@indexing].IdentifiedRentalCost" value="@detail.IdentifiedRentalCost" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedSecurityDeposit_@indexing" name="FurnishedDataList[@indexing].IdentifiedSecurityDeposit" value="@detail.IdentifiedSecurityDeposit" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedMonthlyRentalCost_@indexing" name="FurnishedDataList[@indexing].IdentifiedMonthlyRentalCost" value="@detail.IdentifiedMonthlyRentalCost" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedMonthlyBilling_@indexing" name="FurnishedDataList[@indexing].IdentifiedMonthlyBilling" value="@detail.IdentifiedMonthlyBilling" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedRentalCostPer_@indexing" name="FurnishedDataList[@indexing].IdentifiedRentalCostPer" value="@detail.IdentifiedRentalCostPer" maxlength="800" /></td>

                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedmonthlyMaintenanceCost_@indexing" name="FurnishedDataList[@indexing].IdentifiedmonthlyMaintenanceCost" value="@detail.IdentifiedmonthlyMaintenanceCost" maxlength="100" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedAvgMaintenanceCost_@indexing" name="FurnishedDataList[@indexing].IdentifiedAvgMaintenanceCost" value="@detail.IdentifiedAvgMaintenanceCost" maxlength="100" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedMonthlyElectricCost_@indexing" name="FurnishedDataList[@indexing].IdentifiedMonthlyElectricCost" value="@detail.IdentifiedMonthlyElectricCost" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedMonthlyOtherCosts_@indexing" name="FurnishedDataList[@indexing].IdentifiedMonthlyOtherCosts" value="@detail.IdentifiedMonthlyOtherCosts" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedTotalMonthlyRenatlCost_@indexing" name="FurnishedDataList[@indexing].IdentifiedTotalMonthlyRenatlCost" value="@detail.IdentifiedTotalMonthlyRenatlCost" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedCarpark_@indexing" name="FurnishedDataList[@indexing].IdentifiedCarpark" value="@detail.IdentifiedCarpark" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedName_@indexing" name="FurnishedDataList[@indexing].IdentifiedName" value="@detail.IdentifiedName" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedEmail_@indexing" name="FurnishedDataList[@indexing].IdentifiedEmail" value="@detail.IdentifiedEmail" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedMobile_@indexing" name="FurnishedDataList[@indexing].IdentifiedMobile" value="@detail.IdentifiedMobile" maxlength="10" /></td>
                                            <td> <input type="text" class="form-control form-control-sm input-md makereadonly" id="IdentifiedRemarks_@indexing" name="FurnishedDataList[@indexing].IdentifiedRemarks" value="@detail.IdentifiedRemarks" maxlength="10" /></td>


                                }


                            </tr>
                        }

                    </tbody>
                </table>
                @*</div>*@

                <div class="row" style="margin-left:10px;">


                    <div class="form-group col-lg-6 col-md-6 col-sm-6">
                        <button type="submit" name="Submit" value="StatusUpdate" class="btn btn-primary" style="margin-top:24px"><i class="fa fa-check-circle">&nbsp;</i>Submit and Close Request</button>

                    </div>


                    <div class="form-group col-lg-6 col-md-6 col-sm-6">
                        <button type="submit"  class="btn btn-primary" value="Back" name="Submit" style="margin-top:24px"><i class="fa fa-backward">&nbsp;</i>Back to list</button>

                    </div>

                </div>



            }
            @*</div>*@
        </div>
    </div>
    @*</div>*@


    <script src="~/Scripts/bootstrap/bootstrap-table.min.js"></script>











